<Window x:Class="LASI.UserInterface.Dialogs.CrossJoinSelectDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Documents"
        Height="243"
        Width="302"
        Topmost="True"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize">
    <Grid Margin="0,0,-6,0">

        <Button x:Name="okButton"
                IsEnabled="False"
                Content="OK"
                Margin="63,175,162,0"
                VerticalAlignment="Top"
                Height="20"
                RenderTransformOrigin="-0.878,-0.2"
                Click="okButton_Click" />
        <Button x:Name="cancelButton"
                Content="Cancel"
                Margin="160,175,67,0"
                VerticalAlignment="Top"
                Height="20"
                RenderTransformOrigin="-0.878,-0.2"
                Click="cancelButton_Click" />
        <StackPanel x:Name="documentSelectStackPanel"
                    Margin="0,0,0,45" />

    </Grid>
</Window>
