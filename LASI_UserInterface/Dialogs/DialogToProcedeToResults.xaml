        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Analysis Complete" Height="210" Width="300" WindowStyle="ToolWindow">
    <Grid Height="179" VerticalAlignment="Top">
        <Label Content="&#x9;Analysis Has Been Completed&#xD;&#xA;&#xD;&#xA;            Click Proceed to View the Results" HorizontalAlignment="Left" Margin="10,24,0,91" Width="272"/>
        <CheckBox Content="Automatically Export Results" HorizontalAlignment="Left" Margin="64,103,0,0" VerticalAlignment="Top" Height="18" Width="170" ToolTip="You will be given the option again before closing"/>
        <Button Name="ContinueButton" Content="Continue" HorizontalAlignment="Left" Margin="104,135,0,22" Width="75" Click="ContinueButton_Click"/>

    </Grid>
</Window>
