namespace SharpNLPTaggingModule
{
    /// <summary>
    /// 
    /// </summary>
    public class SharpNLPInvoker
    {



        /// <summary>
        /// The main entry point for the application.
        /// </summary>

        static void Main(string[] args)
        {
