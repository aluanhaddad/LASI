@model LASI.WebService.Models.LoginModel
@{
    ViewBag.Title = "Welcome";
}
@functions{
    public string GetAntiForgeryToken() {
        string cookieToken, formToken;
        AntiForgery.GetTokens(null, out cookieToken, out formToken);
        return cookieToken + ":" + formToken;
    }
}
@using System.Net.Http;
@using System.Web.Http;


<header>
    <h1>@ViewBag.Title</h1>
</header>

<div id="main-content">
    <div class="progress" >
        <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
            60%
        </div>
    </div>
    @using (Ajax.BeginForm("Upload", "Home", new { enctype = "multipart/form-data" }, new AjaxOptions { })) {
        <input id="file" type="file" name="file" multiple />
        <input type="submit" id="Submit" name="Submit" value="Upload" />


    }


    <div>
        @{
            Html.RenderPartial("PartialPage");
        }
    </div>

    @* @("api/jsonservices/5");*@
</div>

@section scripts {

    @*@Scripts.Render("~/bundles/knockout")
        @Scripts.Render("~/bundles/jqueryval")
        @Scripts.Render("~/bundles/jquery")*@
    @*@Scripts.Render("~/bundles/todo")*@
    @Scripts.Render("~/Scripts/app/Index/Index.js")
}





