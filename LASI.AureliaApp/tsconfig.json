{
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "pretty": true,
    "sourceMap": true,
    "diagnostics": false,
    "traceResolution": false,
    "noEmitHelpers": true,
    "importHelpers": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "noImplicitReturns": true,
    "target": "es2016",
    "module": "esnext",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "app": [
        "src"
      ],
      "tslib": [
        "jspm_packages/npm/tslib@1.8.0"
      ]
    }
  },
  "compileOnSave": false
}