{
  "typeAcquisition": {
    "enable": false
  },
  "compileOnSave": false,
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "noEmit": true,
    "module": "es2015",
    "target": "es5",
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noImplicitThis": true,
    "traceResolution": false,
    "pretty": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "lib": [
      "es2017",
      "dom"
    ]
  }
}