{
  "jsRules": {
    "semicolon": [ true, "always" ]
  },
  "rules": {
    "prefer-for-of": true,
    "cyclomatic-complexity": [ true, 25 ],
    "ban": [ [ true, "require", "window", "document" ] ],
    "class-name": true,
    "curly": true,
    "eofline": false,
    "forin": true,
    "indent": [ true, 2 ],
    "interface-name": [ true, "never-prefix" ],
    "label-position": true,
    "max-line-length": [ true, 200 ],
    "no-angle-bracket-type-assertion": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": [ true ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-inferrable-types": [ true ],
    "no-internal-module": true,
    "no-require-imports": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "no-var-requires": true,
    "one-line": [ true, "check-open-brace", "check-catch", "check-finally", "check-else", "check-whitespace" ],
    "quotemark": [ true, "single", "avoid-escape" ],
    "radix": true,
    "semicolon": [ true, "always" ],
    "triple-equals": [ true ],
    "typedef-whitespace": [
      true
    ],
    "variable-name": [ true, "allow-pascal-case", "check-format" ],
    "adjacent-overload-signatures": true,
    "array-type": [ true, "array-simple" ],
    "arrow-parens": [ false ],
    "no-any": true,
    "new-parens": true,
    "one-variable-per-declaration": [ true, "ignore-for-loop" ],
    "use-isnan": true,
    "trailing-comma": [ true ],
    "object-literal-shorthand": true,
    "no-reference": true,
    "no-null-keyword": true,
    "no-unused-new": true,
    "max-classes-per-file": [ true, 200 ],
    "no-unsafe-finally": true,
    "object-literal-key-quotes": [ true, "as-needed" ],
    "whitespace": [ true, "check-branch", "check-decl", "check-module", "check-operator", "check-operator", "check-separator", "check-type" ]
  }
}
