{
  "name": "lasi-ng2-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run lite",
    "lite": "lite-server"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "jspm": "0.17.0-beta.28",
    "jspm-server": "1.0.1",
    "lite-server": "^2.1.0",
    "typings": "^0.8.1"
  },
  "jspm": {
    "main": "app/main.ts",
    "directories": {
      "baseURL": "wwwroot"
    },
    "dependencies": {
      "@angular/forms": "npm:@angular/forms@^0.3.0",
      "@angular/http": "npm:@angular/http@^2.0.0-rc.5",
      "@angular/router": "npm:@angular/router@^3.0.0-rc.1",
      "bootstrap": "github:twbs/bootstrap@^3.3.6",
      "es6-shim": "github:es-shims/es6-shim@^0.35.0",
      "jquery": "npm:jquery@^2.2.3",
      "ng2-conventions-decorators": "npm:ng2-conventions-decorators@^0.1.8"
    },
    "devDependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.13",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^5.0.19",
      "readline": "github:jspm/nodelibs-readline@^0.2.0-alpha",
      "text": "github:systemjs/plugin-text@^0.0.7"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@^2.0.0-rc.5",
      "@angular/compiler": "npm:@angular/compiler@^2.0.0-rc.5",
      "@angular/core": "npm:@angular/core@^2.0.0-rc.5",
      "@angular/platform-browser": "npm:@angular/platform-browser@^2.0.0-rc.5",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.0.0-rc.5",
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "reflect-metadata": "npm:reflect-metadata@^0.1.3",
      "rxjs": "npm:rxjs@5.0.0-beta.6",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zone.js": "npm:zone.js@^0.6.17"
    },
    "overrides": {
      "github:frankwallis/plugin-typescript@5.0.19": {
        "dependencies": {
          "typescript": "npm:typescript@next"
        }
      },
      "github:twbs/bootstrap@3.3.6": {
        "shim": {
          "js/bootstrap": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "2"
        },
        "files": [
          "dist",
          "fonts",
          "js",
          "css",
          "less",
          "grunt",
          "LICENSE"
        ],
        "systemjs": {
          "main": "dist/js/bootstrap.js",
          "modules": {
            "dist/js/bootstrap.js": {
              "deps": [
                "jquery"
              ],
              "exports": "$"
            }
          }
        }
      },
      "npm:@angular/common@2.0.0-rc.5": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/compiler@2.0.0-rc.5": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/core@2.0.0-rc.5": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/http@2.0.0-rc.5": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser-dynamic@2.0.0-rc.5": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:@angular/platform-browser@2.0.0-rc.5": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        }
      },
      "npm:babel-runtime@5.8.38": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@2.2.3": {
        "format": "amd"
      },
      "npm:reflect-metadata@0.1.3": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      }
    }
  }
}
