{

    "app": {
        "options": {
            "report": "gzip",
            "shorthandCompacting": false,
            "roundingPrecision": -1,
            "sourceMap": true,
            "verbose": true
        },
        "src": [ "wwwroot/css/**/*.css" ],
        "dest": "wwwroot/dist/app/app.min.css"
    },
    "lib": {
        "options": {
            "report": "gzip",
            "shorthandCompacting": false,
            "roundingPrecision": -1,
            "sourceMap": true,
            "verbose": true
        },
        "src": [ "wwwroot/lib/**/*.css" ],
        "dest": "wwwroot/dist/lib/lib.min.css"

    }
}
