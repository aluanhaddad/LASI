{
    "compilerOptions": {
        "target": "es5",
        "module": "system",
        "allowJs": true,
        "pretty": true,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noEmit": true,
        "experimentalAsyncFunctions": true,
        "diagnostics": true,
        "watch": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "rootDir": "./"
    },
    "exclude": [
        "jspm_packages"
    ]
}